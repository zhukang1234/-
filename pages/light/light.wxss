/* pages/light/light.wxss */
page {
  background-color: black;
  width:100%;
  height:100%;
  overflow: scroll;
}
.background {
  height: 100%;
  width: 100%;
}

.Color {
  display: flex;
  width: 100%;
  align-items: center;
  justify-content: center;
  flex-direction: row;
  margin-top: 150rpx;
}

.Color_button {
  border: 1px solid white;
}
.title {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
strong{
  font-weight: 700;
  font-size: 15px;
}
subtitle{
  font-size: 10px;
}
.text {
  color: white;
}

.target {
  background-image: url(https://i.postimg.cc/MpShp5bb/test.png);
  background-size: 100% 100%;
  height: 600rpx;
  width: 600rpx;
  margin: 0 auto;
  overflow: hidden;
  border-radius: 50%;
}

.ribbon {
  background: -webkit-linear-gradient(left,grey,white);
  width: 600rpx;
  margin: 40rpx auto;
}

@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 28px;
  line-height: 28px;
  height: 28px;
  width: 28px;
  color: white;
  text-align: center;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-ios-locate-outline:before {
  content: "\ea49";
  color: black;
}


.Color_button_black {
  border: 1px solid white;
}

.tobutton {
  display :flex;
  flex-direction: row;
  background-color: black;
  width: auto;
}

.buttonload {
  color: #fff;
  display: flex;
  width: 100%;
  height: 50rpx;
  align-items: center;
  justify-content: center;
  margin-top: 80rpx;
  background-color:black;
}
