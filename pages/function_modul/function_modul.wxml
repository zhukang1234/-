<!--pages/function_modul/function_modul.wxml-->
<!--left代表左侧按钮，right代表右侧按钮-->
<!--bind开头的为手指冒泡事件详情见https://developers.weixin.qq.com/miniprogram/dev/framework/view/wxml/event.html-->
<view class="left">
  <button class="light" style="width: 25vw; height: 25vw; left:-150rpx" bindtap="Topage2"></button>
</view>
<view class="right">
  <button class="music" style="width: 25vw; height: 25vw; right:-150rpx" bindtap="Topage"></button>
</view>
<view class="left">
  <button class="score" style="width: 25vw; height: 25vw; left:-150rpx" bindtap="Topage1"></button>
</view>
<view class="right">
  <button class="tcp" style="width: 25vw; height: 25vw; right:-150rpx" bindtap="Topage3"></button>
</view>